class MyHashMap {
   int hm[];
    public MyHashMap() {
       hm=new int[1000001];
       Arrays.fill(hm,-1);
    }
    
    public void put(int key, int value) {
        hm[key]=value;
    }
    
    public int get(int key) {
        
        return hm[key];
    }
    
    public void remove(int key) {
        hm[key]=-1;
    }
}
